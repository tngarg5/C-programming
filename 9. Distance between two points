// Distance between two points

#include<stdio.h>
#include<math.h>
struct point{
    int a;
    int b;
} typedef point;

int main()
{
    double dist = 0.0;
   point p1,p2; // 2 points
   printf("Enter point1 and point 2 \n");
   scanf("%d", &p1.a);
    scanf("%d",&p1.b);
    scanf("%d", &p2.a);
    scanf("%d", &p2.b);
    printf ("(%d,%d) & (%d,%d) \n",p1.a,p1.b,p2.a,p2.b);
            
    dist = sqrt(pow((p1.a - p2.a),2) + pow((p1.b-p2.b),2));
    printf("dist between 2 points : %f",dist);
    
    
    
    
    return 0;
    
    
}
    
