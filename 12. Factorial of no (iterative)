// better to use iterative, recursive method can be costly for large numbers
#include <stdio.h>
#include<string.h>

int main() {
    // Write C code here
    int a;
    printf("Enter a number\n");
    scanf("%d", &a);
    int fact =1;
    if(a == 0){
        fact = 1;
    }
    else if(a==1){
        fact = 1;
    }
    else{
    for(int i =2 ; i<= a ; i++){
        fact = fact * i;
    }
    }
    
    printf("ans : %d", fact );
    
  
    
    return 0;
}
