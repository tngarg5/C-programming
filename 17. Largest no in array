// remeber for input in array use loop and &a[i]
#include <stdio.h>

void main()
{
	int  n,m,a,b;
	printf("Enter number of elemnts for arrar:\n");
	scanf("%d", &n);
	printf("Input numbers\n");
	int arr[10];
	for(int i =0; i< n; i++){
	scanf("%d", &arr[i]);
	printf("\t");
	}
	m = arr[0];
	for(int i =1; i<n; i++){
	    if(arr[i] >= m){
	        m = arr[i];
	    }
	}
	printf("largest : %d", m);

	    }
