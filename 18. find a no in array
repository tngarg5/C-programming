#include <stdio.h>

void main()
{
	int  n,x,m = -1;
	printf("Enter number of elemnts for arrar:\n");
	scanf("%d", &n);
	printf("Input numbers\n");
	int arr[10];
	for(int i =0; i< n; i++){
	scanf("%d", &arr[i]);
		}
	printf("Enter no to find");
	scanf("%d", &x);
	
	for(int i =0 ; i<n; i++){
	    if(arr[i] == x){
	        m = i;
	        break;
	    }
	   
	}
	if(m>=0)
	printf("no present at : %d ", m+1);
	else 
	printf("not present  ");
	

	    }
